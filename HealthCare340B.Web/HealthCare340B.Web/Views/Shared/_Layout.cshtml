<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - HealthCare340B</title>
    @* <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" /> *@
    <link rel="stylesheet" href="~/HealthCare340B.Web.styles.css" asp-append-version="true" />

    <link rel="stylesheet" crossorigin href="~/lib/mazer/compiled/css/app.css">
    <link rel="stylesheet" crossorigin href="~/lib/mazer/compiled/css/app-dark.css">
    <link rel="stylesheet" crossorigin href="~/lib/mazer/compiled/css/iconly.css">
    <link rel="stylesheet" href="~/lib/font-awesome/css/all.css" />
</head>
<body>
    <script src="~/lib/mazer/static/js/initTheme.js"></script>
    <div id="app">
        <!-- Sidebar -->
        <partial name="_Sidebar" />
        <div id="main" class='layout-navbar navbar-fixed'>
            <partial name="_Header" />

            <div class="page-heading">
                <h3>@ViewData["Title"]</h3>
            </div>
            <div class="page-content">
                @RenderBody()
            </div>
            <!-- Footer -->
            <footer>

                <div class="footer clearfix mb-0 text-muted">
                    <div class="float-start">
                        <p>2024 &copy; 340B</p>
                    </div>
                    <div class="float-end">
                        <p>
                            Crafted with <span class="text-danger"><i class="bi bi-heart-fill icon-mid"></i></span> by <a href="https://saugi.me">Saugi</a>
                        </p>
                    </div>
                </div>
            </footer>
            
        </div>
    </div>
    @*<script src="~/lib/jquery/dist/jquery.min.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
        <script src="~/js/site.js" asp-append-version="true"></script>*@

    <script src="~/lib/mazer/static/js/components/dark.js"></script>
    <script src="~/lib/mazer/extensions/perfect-scrollbar/perfect-scrollbar.min.js"></script>
    <script src="~/lib/mazer/compiled/js/app.js"></script>

    <!-- Need: Apexcharts -->
    @*<script src="~/lib/mazer/extensions/apexcharts/apexcharts.min.js"></script>
        <script src="~/lib/mazer/static/js/pages/dashboard.js"></script>*@

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
